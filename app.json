{
  "expo": {
    "name": "HipHost",
    "slug": "hip-host",
    "platforms": ["ios", "android"],
    "version": "1.1.12",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splashLoad_bg.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "io.hiphost.app",
      "supportsTablet": false,
      "buildNumber": "1.1.12"
    },
    "android": {
      "package": "com.uth.hiphost",
      "versionCode": 8,
      "splash": {
        "image": "./assets/splashLoad_bg.png",
        "resizeMode": "cover",
        "backgroundColor": "#ffffff"
      },
      "useNextNotificationsApi": true
    },
    "entryPoint": "node_modules/expo/AppEntry.js",
  }
}
